{{#each randomBeer}}
<div class="random-beer-container">
    <div class="random-beer">
        {{> beerPartial this}}
    </div>
    <div class="random-beer-info">
        <h3>Food Pairing: </h3>
        <ul>
            {{#each food_pairing}}
            <li>{{this}}</li>
            {{/each}}
        </ul>
        <p>Brewers Tips: {{brewers_tips}}</p>
    </div>
</div>
{{/each}}